let mapleader=" "
 
:map <leader>tr <Action>(RiderUnitTestRunContextAction)
:map <leader>td <Action>(RiderUnitTestDebugContextAction)
:map <leader>tt <Action>(ActivateUnitTestsToolWindow)
:map <leader>q <Action>(Stop)
:map <leader>f <Action>(ShowIntenionActions)
:map <leader>r <Action>(ChooseRunConfiguration)
 
"" Source your .vimrc
source ~/.vimrc
 
